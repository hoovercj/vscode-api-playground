{
  "name": "vscode-settings-cycler",
  "displayName": "Settings Cycler",
  "description": "Allows cycling through vscode settings using keyboard shortcuts",
  "version": "0.0.2",
  "publisher": "hoovercj",
  "engines": {
    "vscode": "^1.10.0"
  },
  "author": {
    "name": "Cody Hoover",
    "url": "www.codyhoover.com",
    "email": "vscode@codyhoover.com"
  },
  "bugs": {
    "url": "https://www.github.com/hoovercj/vscode-api-playground/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://www.github.com/hoovercj/vscode-api-playground"
  },
  "icon": "images/icon.png",
  "license": "MIT",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "configuration": {
      "properties": {
        "settings.cycle": {
          "type": "array",
          "default": [],
          "items": {
            "type": "object",
            "properties": {
              "setting": {
                "type": "string",
                "description": "The full setting name to change."
              },
              "values": {
                "type": "array",
                "items": {
                  "description": "A valid value for the setting"
                },
                "default": [true, false]
              }
            }
          }
        }
      }
    },
    "commands": [
        {
            "command": "settings.cycle.show",
            "title": "Show settings.cycle"
        }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "deep-equal": "^1.0.1"
  }
}
